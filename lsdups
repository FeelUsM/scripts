#! /bin/bash

if [ $# == 1 -a "$1" == "--no-ls" ]; then
:
else
	echo ll2lpath: >&2
	ll2lpath
fi
filestmp="$(ls -1 .files/20* .files/19* 2>/dev/null | sort -r | head -n 1)"
files="${filestmp%-log}"
#echo $filestmp $files
[ -z "$files" ] && echo can not get files file >&2 && exit 1

runcpp lsdups-internal.cpp "$files" .files/hashes #| runcpp dups-analys.cpp
